{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "operator-linebreak": "off",
        "object-curly-newline": "off",
        "max-len": [
            "error",
            {
                "code": 200
            }
        ]
    }
}
